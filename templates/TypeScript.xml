    </context>
  </template>
  <template name="cls" value="class $classname$ {&#10;    $END$&#10;}" description="Class" toReformat="true" toShortenFQNames="true">
    <variable name="classname" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="edf" value="export default function $name$($args$) {&#10;    $END$&#10;};" description="Export default function" toReformat="true" toShortenFQNames="true">
