<templateSet group="TypeScript">
  <template name="l" value="$ARG$ =&gt; $END$" description="lambda single argument" toReformat="false" toShortenFQNames="true">
    <variable name="ARG" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="ll" value="$ARG$ =&gt; ($END$)" description="lambda multiple arguments" toReformat="false" toShortenFQNames="true">
    <variable name="ARG" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="s" value="() =&gt; $END$" description="lambda no arguments" toReformat="false" toShortenFQNames="true">
    <context>
