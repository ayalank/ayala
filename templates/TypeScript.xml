      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="cc" value="const $VAR$ = '$VALUE$'$END$;" description="const with string" toReformat="false" toShortenFQNames="true">
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
