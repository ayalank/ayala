    <variable name="state" expression="completeSmart()" defaultValue="" alwaysStopAt="true" />
    <variable name="anotherState" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="args" expression="completeSmart()" defaultValue="&quot;mapStateToProps&quot;" alwaysStopAt="true" />
    <variable name="dispatch" expression="" defaultValue="&quot;, {}&quot;" alwaysStopAt="true" />
