      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="cnct" value="connect&lt;$props$, $state$, $anotherState$&gt;($args$$dispatch$)($component$);$END$" description="connect" toReformat="false" toShortenFQNames="true">
